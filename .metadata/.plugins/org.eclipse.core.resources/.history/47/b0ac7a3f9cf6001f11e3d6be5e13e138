package baekjoon;
import java.util.*;
public class ex {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int hour=sc.nextInt();
		int min=sc.nextInt();
		int rehour;
		int remin;
		
		if(hour==0) 
		{
			if(min-45>=0) 
			{
				min -= 45;
				System.out.println(hour +" "+min);
			}
			else
			{
				hour = 23;
				min=60+(min-45);
				System.out.println(hour +" "+min);
			}
		}
		else
		{		
			if(min-45>=0)
			{
				min -= 45;
				System.out.println(hour +" "+remin);
			}
			else 
			{
				rehour = hour - 1;				
				remin = 60 + (min - 45);
				System.out.println(rehour +" "+remin);
			}		
		}	
	}
}
