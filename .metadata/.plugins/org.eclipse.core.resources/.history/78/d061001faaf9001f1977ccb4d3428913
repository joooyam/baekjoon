package baekjoon3;
import java.util.*;
public class B3_2 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		
		for(int i=1; i<10; i++) {
			int a=sc.nextInt();
			for(int j=1; j<=a; j++) {
				int b=sc.nextInt();
				int c=sc.nextInt();
				System.out.print(b+c+" ");
			}
		
			
			}
			
		}
	}

