package baekjoon3;
import java.util.*;
public class B3_9 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		int star=sc.nextInt();
		for(int i=1; i<=star; i++) {
			System.out.println("*");
			for(int j=1; i<=j; j++) {
				System.out.println("*");
			}
		}

	}

}
