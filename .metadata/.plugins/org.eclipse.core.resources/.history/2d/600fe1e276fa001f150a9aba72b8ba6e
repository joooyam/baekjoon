package baekjoon3;
import java.util.*;
public class B3_5 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		
		int a=sc.nextInt();	
		int mat=a/4;
		
		if(a%4==0) {
		for(int i=0; i<=mat; i++) {
			
			
			System.out.println("long"+0+"int");
				
				
			}
			
		}
		
		
	}
}


