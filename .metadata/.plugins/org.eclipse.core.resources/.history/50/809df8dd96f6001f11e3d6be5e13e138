package baekjoon;
import java.util.*;
public class ex {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int hour=sc.nextInt();
		int min=sc.nextInt();
		int rehour;
		int remin;
		
		if(hour==0) 
		{
			hour = 23;
			if(min-45>0) {
				System.out.println(hour +" "+min);
			}else {
				remin=60+(min-45);
				System.out.println(hour +" "+remin);
			}
		}else {
			rehour = hour - 1;
			if(min-45>0) {
				System.out.println(hour +" "+min);
			}else {
				remin=60+(min-45);
				System.out.println(hour +" "+remin);
			}
			
		}
		
	}
}
